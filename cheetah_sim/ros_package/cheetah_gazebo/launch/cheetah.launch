<?xml version="1.0"?>
<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find cheetah_gazebo)/worlds/cheetah.world"/>
	  <arg name="paused" value="true"/>
	  <arg name="gui" default="true"/>
    <!-- arg name="headless" default="true"/ -->
  </include>

  <!-- Load the URDF into the ROS Parameter Server -->
  <param name="robot_description"
    command="$(find xacro)/xacro --inorder '$(find cheetah_description)/urdf/cheetah.urdf'" />

  <node name="spawn_urdf"
   pkg="gazebo_ros"
   type="spawn_model"
   args="-file $(find cheetah_description)/urdf/cheetah.urdf 
   -pause
   -urdf
   -param robot_description 
   -x 0.9 
   -y 0.5 
   -z 0.55 
   -model cheetah" 
   output="screen" /> 


  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find cheetah_gazebo)/display.rviz" output="screen"/>

</launch>

